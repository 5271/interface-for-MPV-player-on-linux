<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="interface_for_MPV_playerFrame">
		<centered>1</centered>
		<size>500,150</size>
		<minsize>500,150</minsize>
		<maxsize>500,150</maxsize>
		<object class="wxButton" name="ID_PLAYBUTTON" variable="PlayButton" member="yes">
			<label>Play the video</label>
			<pos>24,64</pos>
			<size>104,26</size>
			<handler function="OnButton1Click" entry="EVT_BUTTON" />
		</object>
		<object class="wxTextCtrl" name="ID_URLTEXTCTRL" variable="UrlTextCtrl" member="yes">
			<value>Enter video URL here</value>
			<pos>24,24</pos>
			<size>352,27</size>
		</object>
		<object class="wxCheckBox" name="ID_NOAUDIOCHECKBOX" variable="NoAudioCheckBox" member="yes">
			<label>No audio</label>
			<pos>392,28</pos>
		</object>
		<object class="wxCheckBox" name="ID_NOVIDEOCHECKBOX" variable="NoVideoCheckBox" member="yes">
			<label>No video</label>
			<pos>392,56</pos>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
